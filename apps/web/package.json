{
  "name": "@resupaflare/web",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "pnpm run dev:config && pnpm run dev:start",
    "dev:config": "../../scripts/gen-env env-vars > .dev.vars",
    "dev:start": "WORKERS_ENV=development remix dev --no-restart -c \"npm run start\"",
    "start": "wrangler pages dev --env $WORKERS_ENV --compatibility-date=2023-06-21 ./public",
    "lint": "tsc && eslint ."
  },
  "dependencies": {
    "@mantine/core": "7.0.0-alpha.18",
    "@mantine/hooks": "7.0.0-alpha.18",
    "@remix-run/cloudflare": "^1.18.1",
    "@remix-run/cloudflare-pages": "^1.18.1",
    "@remix-run/css-bundle": "^1.18.1",
    "@remix-run/react": "^1.18.1",
    "@resupaflare/db": "workspace:^",
    "@supabase/auth-helpers-remix": "^0.2.1",
    "@supabase/supabase-js": "^2.26.0",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.18.1",
    "@resupaflare/tsconfig": "workspace:*",
    "@types/node": "^20.4.2",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "postcss": "^8.4.26",
    "postcss-preset-mantine": "^1.7.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.0.4",
    "wrangler": "^3.1.1"
  },
  "engines": {
    "node": ">=16.13.0"
  }
}
